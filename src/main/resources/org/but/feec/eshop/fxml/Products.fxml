                        <TableView fx:id="systemProductTableView" layoutX="12.0" layoutY="57.0" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="402.0" prefWidth="888.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="-308.0">
